const unsigned long eventInterval = 120;
unsigned long previousTime = 0;
int num=90;
int numstep=10;
#include <Servo.h>
Servo servo;
void setup() {
  Serial.begin(9600);
  servo.attach(9);
}

void loop() {
  unsigned long currentTime = millis();

  if (currentTime - previousTime >= eventInterval){
    servo.write(num);
    num=num+numstep;
    if(num<=85||num>=115){
  numstep=-1*numstep;
}

    previousTime = currentTime;
  }

}
